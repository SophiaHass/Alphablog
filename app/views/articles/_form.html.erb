  <div class="col-10">

  <%= render "shared/errors" %>

    <%= form_with(model: @article, local: true) do |f| %>
    <div class="form-group">
      <%= f.label :title, class: "call-2 call-form-label" %> 
        
      <%= f.text_field :title, class: "form-control" %>
        
    </div>

    <div class="form-group">
      <%= f.label :description, class: "call-2 call-form-label" %> <br>
        
          <%= f.text_area :description, rows: 10, class: "form-control" %>
        
    </div>

    <div class="form-group">
      <%= f.submit class: "btn-warning btn-lg" %>
    </div>

    <% end %>

    </div>
    <%= render "backto" %>
  </div>

</div>